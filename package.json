  "scripts": {
    "clean": "rm -r build/ts-to-es6 && rm -r build/bundles",
    "transpile:sources": "$(yarn bin)/tsc --project build/config/tsconfig.json",
    "transpile:tests": "$(yarn bin)/tsc --project build/config/tsconfig.tests.json",
    "bundle:sources": "TESTS=false $(yarn bin)/rollup --config build/config/rollup.config.js",
    "bundle:sources:webpack": "TESTS=false $(yarn bin)/webpack --config build/config/webpack.config.js --profile --json > stats.json",
    "bundle:tests": "TESTS=true $(yarn bin)/rollup --config build/config/rollup.config.js",
    "build:sources:dev": "ENV=development yarn transpile:sources && ENV=development yarn bundle:sources",
    "build:sources:webpack:dev": "ENV=development yarn transpile:sources && ENV=development yarn bundle:sources:webpack",
    "build:sources:staging": "ENV=staging yarn transpile:sources && ENV=staging yarn bundle:sources",
    "build:sources:prod": "ENV=production yarn transpile:sources && ENV=production yarn bundle:sources",
    "build:sources:webpack:prod": "ENV=production yarn transpile:sources && ENV=production yarn bundle:sources:webpack",
    "build:tests": "ENV=development yarn transpile:tests",
    "build": "yarn build:sources:dev",
    "publish:dev": "ENV=development build/scripts/publish.sh",
    "publish:staging": "ENV=staging build/scripts/publish.sh",
    "publish:prod": "ENV=production build/scripts/publish.sh",
    "test": "ava --verbose --color --watch"
  },
  "config": {
    "sdkVersion": "150106",
    "onesignalPublicPath": "/Users/jpang/code/OneSignal/public",
    "onesignalPublicSdksPath": "/Users/jpang/code/OneSignal/public/sdks"
  },
  "repository": {
    "type": "git",
    "url": "github.com/one-signal/OneSignal-Website-SDK"
