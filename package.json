  "ava": {
    "files": [
      "build/ts-to-es6/test/unit/**/*.js"
    ],
    "source": [
      "build/ts-to-es6/src/**/*.js"
    ],
    "concurrency": 1,
    "serial": true,
    "powerAssert": true,
    "watch": true,
    "failFast": true
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "build/javascript/src/**/*.js"
    ]
  }
